{
	"name": "layercake",
	"version": "8.3.4",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit sync && svelte-package -o dist && publint",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"update_template": "sh ./src/scripts/update_template.sh",
		"prebuild": "npm run update_template",
		"deploy": "npm run build && git add -A && git commit -am 'auto-deploy' && git push origin main",
		"test": "mocha --require esm test/*.test.js"
	},
	"license": "MIT",
	"type": "module",
	"devDependencies": {
		"@rollup/plugin-dsv": "^3.0.4",
		"@sveltejs/adapter-static": "^3.0.3",
		"@sveltejs/kit": "^2.5.21",
		"@sveltejs/package": "^2.3.3",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/cookie": "^0.6.0",
		"@types/d3": "^7.4.3",
		"@types/markdown-it": "^14.1.2",
		"@types/node": "^22.2.0",
		"cookie": "^0.6.0",
		"d3-collection": "^1.0.7",
		"d3-delaunay": "^6.0.4",
		"d3-force": "^3.0.0",
		"d3-format": "^3.1.0",
		"d3-geo": "^3.1.1",
		"d3-hierarchy": "^3.1.2",
		"d3-quadtree": "^3.0.1",
		"d3-sankey": "^0.12.3",
		"d3-time": "^3.1.0",
		"d3-time-format": "^4.1.0",
		"degit": "^2.8.4",
		"do-not-zip": "^1.0.0",
		"doctrine": "^3.0.0",
		"emoji-regex": "^10.3.0",
		"eslint": "^9.9.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"esm": "^3.2.25",
		"golden-fleece": "^1.0.9",
		"highlight.js": "^11.10.0",
		"indian-ocean": "^4.0.3",
		"markdown-it": "^14.1.0",
		"marked": "^14.0.0",
		"mocha": "^10.7.3",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.9",
		"regl": "^2.1.0",
		"svelte": "5.0.0-next.216",
		"svelte-check": "^3.8.5",
		"svelte-preprocess": "^6.0.2",
		"svelte2tsx": "^0.7.15",
		"topojson-client": "^3.1.0",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"underscore": "^1.13.7",
		"vite": "^5.4.0",
		"yootils": "^0.3.1"
	},
	"dependencies": {
		"d3-array": "^3.2.4",
		"d3-color": "^3.1.0",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0",
		"globals": "^15.9.0"
	},
	"main": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "3 - 5 || >=5.0.0-next.120",
		"typescript": "^5.0.2"
	},
	"svelte": "./dist/index.js"
}
